{
  "projectType": "application",
  "root": "apps/angular-client",
  "sourceRoot": "apps/angular-client/src",
  "prefix": "ticketing",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/angular-client",
        "index": "apps/angular-client/src/index.html",
        "main": "apps/angular-client/src/main.ts",
        "polyfills": "apps/angular-client/src/polyfills.ts",
        "tsConfig": "apps/angular-client/tsconfig.app.json",
        "assets": [
          "apps/angular-client/src/favicon.ico",
          "apps/angular-client/src/assets"
        ],
        "styles": ["apps/angular-client/src/styles.css"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "libs/shared/env/src/lib/environment.ts",
              "with": "libs/shared/env/src/lib/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "staging": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "libs/shared/env/src/lib/environment.ts",
              "with": "libs/shared/env/src/lib/environment.staging.ts"
            }
          ],
          "outputHashing": "all"
        },
        "local": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "libs/shared/env/src/lib/environment.ts",
              "with": "libs/shared/env/src/lib/environment.local.ts"
            }
          ]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "angular-client:build:production"
        },
        "development": {
          "browserTarget": "angular-client:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "angular-client:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/angular-client/src/**/*.ts",
          "apps/angular-client/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/angular-client"],
      "options": {
        "jestConfig": "apps/angular-client/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:frontend", "type:app", "platform:web"]
}
