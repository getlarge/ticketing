{
  "openapi": "3.0.0",
  "paths": {
    "/api/users/on-sign-up": {
      "post": {
        "operationId": "UsersController_onSignUp",
        "summary": "Register a user - Scope : users:create_one",
        "description": "Triggered when a user is created in Ory",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OnOrySignUpDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OnOrySignUpDto"
                }
              }
            }
          }
        },
        "tags": [
          "users"
        ]
      }
    },
    "/api/users/on-sign-in": {
      "post": {
        "operationId": "UsersController_onSignIn",
        "summary": "Login a user - Scope : users:sign_in",
        "description": "Triggered when a user signed in via Ory",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OnOrySignInDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User logged in",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OnOrySignInDto"
                }
              }
            }
          }
        },
        "tags": [
          "users"
        ]
      }
    },
    "/api/users/sign-up": {
      "post": {
        "operationId": "UsersController_signUp",
        "summary": "Register a user - Scope : users:create_one",
        "description": "Request creation of a user",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserCredentialsDto"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "User created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserDto"
                }
              }
            }
          }
        },
        "tags": [
          "users"
        ]
      }
    },
    "/api/users/current-user": {
      "get": {
        "operationId": "UsersController_getCurrentUser",
        "summary": "Get current user - Scope : users:read_one",
        "description": "Get details about currently signed in user",
        "parameters": [],
        "responses": {
          "201": {
            "description": "Current user logged in",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserDto"
                }
              }
            }
          }
        },
        "tags": [
          "users"
        ],
        "security": [
          {
            "session": []
          },
          {
            "bearer": []
          }
        ]
      }
    },
    "/api/clients": {
      "post": {
        "operationId": "ClientsController_create",
        "summary": "Register a new client - Scope : clients:create_one",
        "description": "Register a new client",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateClientDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Client created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ClientDto"
                }
              }
            }
          }
        },
        "tags": [
          "clients"
        ]
      }
    },
    "/api/clients/on-token-request": {
      "post": {
        "operationId": "ClientsController_onSignUp",
        "summary": "",
        "description": "Triggered when a client request an OAuth2 token",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OryOAuth2WebhookPayloadDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Token session update",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OryOAuth2WebhookResponseDto"
                }
              }
            }
          }
        },
        "tags": [
          "clients"
        ]
      }
    },
    "/api/clients/current-client": {
      "get": {
        "operationId": "ClientsController_getCurrentClient",
        "summary": "Get current client - Scope : clients:read_one",
        "description": "Get details about currently authenticated client",
        "parameters": [],
        "responses": {
          "201": {
            "description": "Current client authenticated",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ClientDto"
                }
              }
            }
          }
        },
        "tags": [
          "clients"
        ],
        "security": [
          {
            "bearer": []
          }
        ]
      }
    }
  },
  "info": {
    "title": "Auth API",
    "description": "Ticketing auth API description",
    "version": "0.2.0",
    "contact": {}
  },
  "tags": [
    {
      "name": "users",
      "description": ""
    },
    {
      "name": "clients",
      "description": ""
    }
  ],
  "servers": [
    {
      "url": "http://localhost:3000"
    },
    {
      "url": "http://localhost:8080"
    }
  ],
  "components": {
    "securitySchemes": {
      "session": {
        "type": "apiKey",
        "in": "cookie",
        "name": "session"
      },
      "bearer": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "OryIdentityTraitDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      },
      "OryIdentityDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "schema_id": {
            "type": "string"
          },
          "schema_url": {
            "type": "string"
          },
          "traits": {
            "description": "The identity traits",
            "allOf": [
              {
                "$ref": "#/components/schemas/OryIdentityTraitDto"
              }
            ]
          },
          "state": {
            "type": "object"
          },
          "recovery_addresses": {
            "type": "object"
          },
          "verifiable_addresses": {
            "type": "object"
          },
          "credentials": {
            "type": "object"
          },
          "metadata_public": {
            "type": "object"
          },
          "metadata_admin": {
            "type": "object"
          },
          "created_at": {
            "type": "string"
          },
          "updated_at": {
            "type": "string"
          },
          "state_changed_at": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "schema_id",
          "schema_url",
          "traits"
        ]
      },
      "OnOrySignUpDto": {
        "type": "object",
        "properties": {
          "identity": {
            "description": "The identity created by Ory",
            "allOf": [
              {
                "$ref": "#/components/schemas/OryIdentityDto"
              }
            ]
          }
        },
        "required": [
          "identity"
        ]
      },
      "OnOrySignInDto": {
        "type": "object",
        "properties": {
          "identity": {
            "description": "The identity logged in Ory",
            "allOf": [
              {
                "$ref": "#/components/schemas/OryIdentityDto"
              }
            ]
          }
        },
        "required": [
          "identity"
        ]
      },
      "UserCredentialsDto": {
        "type": "object",
        "properties": {
          "password": {
            "type": "string",
            "description": "User password",
            "format": "password",
            "minLength": 4,
            "maxLength": 20
          },
          "email": {
            "type": "string",
            "description": "User email address",
            "format": "email"
          }
        },
        "required": [
          "password",
          "email"
        ]
      },
      "UserDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "User email address",
            "format": "email"
          },
          "id": {
            "type": "string",
            "description": "User id"
          },
          "identityId": {
            "type": "string",
            "description": "User identity id",
            "format": "uuid"
          }
        },
        "required": [
          "email",
          "id",
          "identityId"
        ]
      },
      "CreateClientDto": {
        "type": "object",
        "properties": {}
      },
      "ClientDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "clientId": {
            "type": "string",
            "description": "Ory client id",
            "format": "uuid"
          },
          "userId": {
            "type": "string",
            "description": "Owner id"
          }
        },
        "required": [
          "id",
          "clientId",
          "userId"
        ]
      },
      "OryOAuth2WebhookPayloadDto": {
        "type": "object",
        "properties": {}
      },
      "OryOAuth2WebhookResponseDto": {
        "type": "object",
        "properties": {}
      }
    }
  },
  "security": [
    {
      "session": []
    },
    {
      "bearer": []
    }
  ]
}