{
  "angular.enable-strict-mode-prompt": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true
  },
  "yaml.schemas": {
    "https://raw.githubusercontent.com/ory/keto/master/.schema/version.schema.json": [
      "keto*.yml",
      "keto*.yaml"
    ],
    "https://raw.githubusercontent.com/ory/kratos/master/.schema/version.schema.json": [
      "kratos*.yml",
      "kratos*.yaml"
    ],
    "https://raw.githubusercontent.com/ory/hydra/master/.schema/version.schema.json": [
      "hydra*.yml",
      "hydra*.yaml"
    ]
  }
}
