{
  "root": "libs/microservices/shared/middlewares",
  "sourceRoot": "libs/microservices/shared/middlewares/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/microservices/shared/middlewares/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/microservices/shared/middlewares"],
      "options": {
        "jestConfig": "libs/microservices/shared/middlewares/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:shared", "type:util", "platform:server"]
}
