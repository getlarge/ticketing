    "@nrwl/react": "13.2.4",
    "@nrwl/tao": "13.2.4",
    "@nrwl/web": "13.2.4",
    "@nrwl/workspace": "13.2.4",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/bull": "^3.15.6",
    "@types/jest": "27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.12",
